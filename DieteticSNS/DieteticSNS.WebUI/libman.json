{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "font-awesome@5.10.2",
      "destination": "wwwroot/lib/font-awesome/"
    },
    {
      "library": "jquery@3.4.1",
      "destination": "wwwroot/lib/jquery/"
    },
    {
      "library": "jquery-validate@1.19.1",
      "destination": "wwwroot/lib/jquery-validate/"
    },
    {
      "library": "jquery-validation-unobtrusive@3.2.11",
      "destination": "wwwroot/lib/jquery-validation-unobtrusive/"
    },
    {
      "library": "datatables@1.10.19",
      "destination": "wwwroot/lib/datatables/"
    },
    {
      "library": "bootstrap-select@1.13.11",
      "destination": "wwwroot/lib/bootstrap-select/",
      "files": [
        "css/bootstrap-select.min.css",
        "css/bootstrap-select.css.map",
        "css/bootstrap-select.css",
        "js/bootstrap-select.js",
        "js/bootstrap-select.js.map",
        "js/bootstrap-select.min.js",
        "js/bootstrap-select.min.js.map"
      ]
    },
    {
      "provider": "jsdelivr",
      "library": "jquery-ajax-unobtrusive@3.2.6",
      "destination": "wwwroot/lib/jquery-ajax-unobtrusive/",
      "files": [
        "dist/jquery.unobtrusive-ajax.js",
        "dist/jquery.unobtrusive-ajax.min.js"
      ]
    },
    {
      "provider": "unpkg",
      "library": "@microsoft/signalr@latest",
      "destination": "wwwroot/lib/signalr/",
      "files": [
        "dist/browser/signalr.js",
        "dist/browser/signalr.min.js"
      ]
    }
  ]
}